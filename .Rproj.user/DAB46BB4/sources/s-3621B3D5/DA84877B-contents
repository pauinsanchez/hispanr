\name{provmap}
\alias{provmap}
\title{Choropleth map for current Spanish provinces}
\usage{
provmap(data, a, color)
}
\arguments{
\item{data}{A dataframe containing the variable assigned to each province}

\item{a}{A vector specifying the breakpoints of the desired choropleth intervals}

\item{color}{A ColorBrewer sequential palette}
}
\description{
This function creates a choropleth map for Spanish provinces
}
\examples{
provmap(data, c(0, 10, 20, 30, 40), "Reds")
}
\keyword{choropleth}
\keyword{maps}
\keyword{spain}
